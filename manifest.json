{
  "name": "CipherMemo",
  "version": "1.0",
  "description": "A secure note-taking extension",
  "manifest_version": 3,
  "author": "Shivam Agarwal & Piyush Verma",
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "48": "assets/icon48.png",
    "128": "assets/icon128.png"
  }, 
  "action": {
    "default_popup": "register.html",
    "default_title": "CipherMemo",
    "default_icon": {
      "16": "assets/icon16.png",
      "32": "assets/icon32.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png"
    }
  },
  "permissions": [
    "storage"
  ]
}
